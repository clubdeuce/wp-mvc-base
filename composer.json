{
	"name": "clubdeuce/wpmvcbase",
	"type": "wordpress-muplugin",
	"description": "MVC style framework for WordPress plugin development",
	"keywords": ["wordpress","mvc","plugins"],
	"homepage": "https://github.com/clubdeuce/wp-mvc-base",
	"license": "GPLv2.0",
	"authors": [
		{
			"name": "ClubDeuce",
			"email": "daryl@clubdeuce.ninja",
			"role": "Developer"
    	}
	],
	"require": {
		"php": ">=5.2"
	},
	"repositories": [
	    {
	        "type": "package",
	        "package": {
	            "name": "sebastianbergmann/phpcov",
	            "version": "1.1.0",
	            "dist": {
	                "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
	                "type": "zip"
	            },
	            "source": {
	                "url": "https://github.com/sebastianbergmann/phpcov.git",
	                "type": "git",
	                "reference": "1.1.0"
	            },
	            "bin": [
	                "phpcov.php"
	            ]
	        }
	    }
	],
	"require-dev": {
	    "satooshi/php-coveralls": "dev-master",
	    "sebastianbergmann/phpcov": "1.1.0",
	    "mikey179/vfsStream": "1.5.0",
        "phpunit/phpunit": "4.6.6",
        "mockery/mockery": "0.9.2",
        "phpmd/phpmd": "2.2.0"
	}
}
